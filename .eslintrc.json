{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "globals": {
  },
  "parserOptions": {
  },
  "plugins": [
  ],
  "rules": {
      "no-console": ["warn", {"allow": ["info", "warn", "error"]}],
      "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
      "no-undef": ["warn"]
  }
}